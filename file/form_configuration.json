                  },
                  {  
                     "key":105,
                     "value":"Not Departed"
                  }
               ],
               "validation":"r"
            },
            {  
               "labelkey":"containerInventory.containerStatus",
               "type":"radio",
               "name":"containerStatus",
               "tabindex":"7",
               "switch":true,
               "defaultvalue":[  
                  {  
                     "labelkey":"containerInventory.containerStatus.active",
                     "value":"active"
                  },
                  {  
                     "labelkey":"containerInventory.containerStatus.inActive",
                     "value":"InActive"
                  }
               ],
               "validation":"r"
            },
            {  
               "labelkey":"containerInventory.daysOfGP",
               "type":"checkbox",
               "name":"daysOfGP",
               "tabindex":"8",
               "defaultvalue":[  
                  {  
                     "label":"containerInventory.daysOfGP.Days",
                     "value":"days"
                  },
                  {  
                     "label":"containerInventory.daysOfGP.Week",
                     "value":"week"
                  },
                  {  
                     "label":"containerInventory.daysOfGP.Month",
                     "value":"month"
                  }
               ],
               "validation":"r"
            },
            {  
               "labelkey":"containerInventory.vgmMeasuredDate",
               "type":"text",
               "name":"vgmMeasuredDate",
               "tabindex":"9",
               "format":"date",
               "defaultvalue":"",
               "dateFormat":"d/m/yyyy",
               "minDate":"24/12/2015",
               "validation":"r"
            },
            {  
               "labelkey":"containerInventory.containerImage",
               "type":"file",
               "name":"containerImage",
               "tabindex":"10",
               "multiple":true,
               "validation":"r",
               "image":{  
                  "accept":"jpg,png",
                  "size":"5MB"
               }
            }
         ],
         "formButtons":[  
            {  
               "submit":{  
                  "keyname":"btnSubmit",
                  "keyvalue":"containerInventory.btnAddContainerInventory"
               },
               "reset":{  
                  "keyname":"btnReset",
                  "keyvalue":"containerInventory.btnResetContainerInventory"
               },
               "cancel":{  
                  "keyname":"btnCancel",
                  "keyvalue":"containerInventory.btnCancelContainerInventory"
               }
            }
