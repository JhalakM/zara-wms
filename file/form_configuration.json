{  
   "errorcode":0,
   "returnobject":{  
      "forminfo":{  
         "caption":"Add Container Inventory",
         "formTag":{  
            "method":"POST",
            "name":"addContainerInventory",
            "action":"../containerInventory/addContainerInventory"
         },
         "formElements":[  
            {  
               "labelkey":"containerInventory.containerNumber",
               "type":"text",
               "name":"containerNumber",
               "tabindex":"1",
               "defaultvalue":"",
               "validation":"r"
            },
            {  
               "labelkey":"containerInventory.containerCycle",
               "type":"text",
               "name":"containerCycle",
               "tabindex":"2",
               "defaultvalue":"",
               "validation":"r,n"
            },
            {  
               "labelkey":"containerInventory.email",
               "type":"text",
               "name":"email",
               "tabindex":"3",
               "defaultvalue":"",
               "validation":"r,e"
            },
            {  
               "labelkey":"containerInventory.vgm",
               "type":"text",
               "name":"vgm",
               "tabindex":"4",
               "defaultvalue":"",
               "validation":"r,n"
            },
            {  
               "labelkey":"containerInventory.address",
               "type":"textarea",
               "name":"address",
               "tabindex":"5",
               "rows":"10",
               "cols":"5",
               "defaultvalue":"",
